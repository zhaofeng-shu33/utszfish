/**app.wxss**/
@import 'style/weui.wxss';
@import 'style/ezui.wxss';
@import 'style/iconfont.wxss';

page{
  width: 100%;
  height: 100%;
  font-family: Verdana;
   /* background-color: #f1f1f1;*/
  background: linear-gradient(#fdda42,#f1f1f1) ;
  background-attachment: fixed;
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
view{
  box-sizing: border-box;
}